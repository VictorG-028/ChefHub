{
  "name": "chefhub-backend",
  "version": "1.0.0",
  "description": "Backend do projeto ChefHub. Utiliza TypeScript, NodeJS, Express, Nodemon, Concurrently, Jest e SuperTest. Faz uso do banco de dados Supabase.",
  "scripts": {
    "import_db_changes_from_remote": "npx supabase db remote commit",
    "apply_db_changes": "npx supabase db reset",
    "gen_supabase_types": "npx supabase gen types typescript --local > src/lib/database.types.ts",
    "update_db": "npx supabase db remote commit && npx supabase db reset && npx supabase gen types typescript --local > src/lib/database.types.ts",
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\"",
    "remove_dist_insides": "rm -rf dist/*",
    "test": "jest --verbose --detectOpenHandles",
    "JSON comment(dont run this)": "SET USE_TEST_ENV='true' && jest --watchAll --verbose --coverage"
  },
  "keywords": [],
  "author": "VictorG-028",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.4.1",
    "@types/pluralize": "^0.0.30",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.0",
    "supabase": "^1.77.9",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-mockito": "^2.6.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.26.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "bimg": "^0.1.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "json5": "^2.2.3",
    "openai": "^3.3.0",
    "pluralize": "^8.0.0",
    "uuid": "^9.0.0"
  }
}
